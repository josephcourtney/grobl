{
  "project_types": {
    "python": ["requirements.txt"],
    "javascript": ["package.json"],
    "typescript": ["tsconfig.json"],
    "rust": ["Cargo.toml"],
    "wasm": ["wasm"]
  },
  "ignore_patterns": {
    "python": {
      "exclude_tree": ["*.pyc"],
      "exclude_print": []
    },
    "javascript": {
      "exclude_tree": ["node_modules/*"],
      "exclude_print": []
    },
    "typescript": {
      "exclude_tree": ["dist/*"],
      "exclude_print": []
    },
    "rust": {
      "exclude_tree": ["target/*"],
      "exclude_print": []
    },
    "wasm": {
      "exclude_tree": ["*.wasm"],
      "exclude_print": []
    }
  }
}
