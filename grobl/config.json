{
  "project_types": {
    "python": ["venv", "conda", "pytest", "mypy", "ruff"],
    "javascript": ["node_modules", "yarn.lock", "jest", "eslint"],
    "typescript": ["node_modules", "yarn.lock", "jest", "eslint", "tsconfig.json"],
    "rust": ["Cargo.toml", "target"],
    "wasm": ["wasm"]
  },
  "ignore_patterns": {
    "python": {
      "exclude_tree": [
        "*\\.jsonl",
        "*\\.jsonl\\.*",
        "cov\\.xml",
        "*\\.log",
        "*\\.tmp",
        "*/build/*",
        "*/_build/*",
        "*/docs/*",
        "*/coverage/*",
        "*/__pycache__/*",
        "*/venv/*",
        "*/\\.venv/*",
        "mypy_cache/*",
        "\\.pytest_cache/*",
        "*\\.pyc",
        "*\\.pyo"
      ],
      "exclude_print": []
    },
    "javascript": {
      "exclude_tree": [
        "*\\.jsonl",
        "*\\.jsonl\\.*",
        "cov\\.xml",
        "*\\.log",
        "*\\.tmp",
        "*/build/*",
        "*/_build/*",
        "*/docs/*",
        "*/coverage/*",
        "*/node_modules/*",
        "*/__mocks__/*",
        "*/package-lock\\.json"
      ],
      "exclude_print": []
    },
    "typescript": {
      "exclude_tree": [
        "*\\.jsonl",
        "*\\.jsonl\\.*",
        "cov\\.xml",
        "*\\.log",
        "*\\.tmp",
        "*/build/*",
        "*/_build/*",
        "*/docs/*",
        "*/coverage/*",
        "*/node_modules/*",
        "*/__mocks__/*",
        "*/package-lock\\.json",
        "*/dist/*",
        "*/tsconfig\\.tsbuildinfo"
      ],
      "exclude_print": []
    },
    "rust": {
      "exclude_tree": [
        "*\\.jsonl",
        "*\\.jsonl\\.*",
        "cov\\.xml",
        "*\\.log",
        "*\\.tmp",
        "*/build/*",
        "*/_build/*",
        "*/docs/*",
        "*/target/*",
        "Cargo\\.lock"
      ],
      "exclude_print": []
    },
    "wasm": {
      "exclude_tree": [
        "*\\.jsonl",
        "*\\.jsonl\\.*",
        "cov\\.xml",
        "*\\.log",
        "*\\.tmp",
        "*/build/*",
        "*/_build/*",
        "*/docs/*",
        "*/target/*",
        "*\\.wasm",
        "*\\.wat"
      ],
      "exclude_print": ["wasm"]
    }
  }
}
